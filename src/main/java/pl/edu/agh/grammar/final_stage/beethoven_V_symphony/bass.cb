melody bass() {
    SET INSTRUMENT=BASS;
    PAUSE 5;

    PLAY G0 100 G0 150 G0 200 Eb0 1500;
    PAUSE 1500;
    PLAY F0 100 F0 150 F0 200 D0 1500;
    PAUSE 2100;

    PLAY Eb0 800 G0 800;
    PLAY C0 600;
    PAUSE 850;
    PLAY D0 800 G0 800;
    PLAY D0 600;
    PAUSE 850;

    PLAY C0 800 H-1 800 C0 800;

    PAUSE 400;
    PLAY H0 H0 H0 400 C0 800;
    PAUSE 400;
    PLAY C0 800;
    PAUSE 400;
    PLAY G0 1000;
    PAUSE 1000;

    PLAY Ab0 Ab0 Ab0 400 F0 1200;
    PAUSE 250;
}